{
    "version": "0.2.0",
     "configurations": [
        {
            "name": "attach to qemu-static",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/target/thumbv7em-none-eabihf/debug/myrustyprogram",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb-multiarch",
            "miDebuggerServerAddress": "localhost:3333"
        }
    ]
}